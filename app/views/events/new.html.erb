<%= turbo_frame_tag "modal" do %>
  <div class="mt-6" data-controller="turbo-modal" data-action="turbo:submit-end->turbo-modal#submitEnd">
    <h1 class="inline-block mb-2 font-extrabold text-3xl tracking-tight text-slate-800 dark:text-white">Create a new event:</h1>
    <%= render "form", event: @event %>
  </div>
<% end %>
